<h1>Login!</h1>

{% if error %}
    <div>{{ error.messageKey | trans(error.messageData, "security") }}</div>
{% endif %}

{{ form_start(loginForm) }}
    {{ form_row(loginForm._username) }}
    {{ form_row(loginForm._password) }}
    <input type="submit" class="button" value="Login">
{{ form_end(loginForm) }}
